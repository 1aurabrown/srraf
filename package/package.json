{
  "name": "srraf",
  "version": "1.0.0",
  "description": "Better scroll and resize handling",
  "main": "dist/index.js",
  "scripts": {
    "umd": "mkdir -p dist && browserify index.js --standalone overunder > dist/index.umd.js",
    "lint": "standard 'src/*.js'",
    "prebuild": "npm run lint",
    "build": "babel index.js -d dist/",
    "watch": "NODE_ENV=development babel -w --no-comments index.js -d dist/"
  },
  "browserify": {
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/srraf.git"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/srraf/issues"
  },
  "homepage": "https://github.com/estrattonbailey/srraf#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "minifyify": "^7.3.5",
    "standard": "^8.6.0",
    "watchify": "^3.9.0"
  }
}
